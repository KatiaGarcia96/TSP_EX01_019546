---Consulta 1

SELECT CUS.FIRST_NAME,CUS.LAST_NAME, CUS.EMAIL_ADDRESS FROM EX1_019546.CUSTOMER_TABLE CUS
WHERE COMPANY_NAME= 'XXXX';


---Consulta 2

SELECT CUS.COMPANY_NAME FROM EX1_019546.CUSTOMER_TABLE CUS
INNER JOIN EX1_019546.ADDRESS_TABLE ADDR
ON CUS.ID_ADDRESS=ADDR.ID_ADDRESS
INNER JOIN EX1_019546.CITY_TABLE CIT
ON ADDR.ID_CITY=CIT.ID_CITY
WHERE NAME_CITY='XXXXX';

